{
  "extends": [
    "eslint:recommended",
    "plugin:flowtype/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "flowtype"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "flowtype/generic-spacing": 0
  }
}
